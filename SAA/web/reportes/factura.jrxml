	<parameter name="Folfact" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT
	F.F_IdFact,
	U.F_NomCli,
	DATE_FORMAT(F.F_FecEnt, '%d/%m/%Y') AS F_FecEnt,
	F.F_ClaDoc,
	F.F_ClaPro,
	M.F_DesPro,
	Lotes.F_ClaLot,
	Lotes.F_FecCad,
	F.F_CantReq,
	F.F_CantSur,
	F.F_Costo,
	F.F_Iva,
	F.F_Monto,
	F.F_Ubicacion,
	(@csum := F.F_Monto +@csum) AS totales
FROM
	(SELECT @csum := 0) r,
	(
		SELECT
			L.F_FolLot,
			L.F_ClaLot,
			DATE_FORMAT(L.F_FecCad, '%d/%m/%Y') AS F_FecCad
		FROM
			tb_lote L
		GROUP BY
			L.F_FolLot
	) Lotes,
	tb_factura F,
	tb_medica M,
	tb_uniatn U
WHERE
	F.F_ClaPro = M.F_ClaPro
AND F.F_ClaCli = U.F_ClaCli
AND F.F_lote = Lotes.F_FolLot
AND F.F_ClaDoc = $P{Folfact}
ORDER BY
	totales]]>
	</queryString>
